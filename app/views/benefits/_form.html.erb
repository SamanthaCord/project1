<%= form_for @benefit do | f | %>
  <fieldset>
      <%= f.label :name, :class => 'benefitnamelabel' %>
      <%= f.text_field :name, :required => true, :placeholder => '  Problem area or health kick  Eg. Immunity' %>
  </fieldset>
  <label></label>
  <fieldset>
    <%= f.submit "Add Item", :class => 'addbenefitbutton' %>
  </fieldset>
<% end %>
